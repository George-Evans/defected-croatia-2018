/*------------------------------
LINEUP
------------------------------*/
ul.lineup {
	list-style: none;
	width: auto;
	margin: 0 auto;
	padding: 0 0;
	text-align: center;
	cursor: auto;
}
ul.lineup li {
	height: auto;
	width: auto;
	display: inline-block;
	cursor: pointer;
	font-family: 'ITCAvantGardeGothW04-Bo 1122872', "Helvetica Neue", Arial;
	color: #FFF;
	letter-spacing: -1px;
	line-height:100%;
	padding: 0 0;
	margin: 0 0;
	cursor: auto;
}
ul.lineup li a {
	color: #FFF;
	text-decoration:none;
	-o-transition: .6s ease-in-out;
	-ms-transition: .6s ease-in-out;
	-moz-transition: .6s ease-in-out;
	-webkit-transition: .6s ease-in-out;
	transition: .6s ease-in-out;
	cursor: auto;
}
ul.lineup li a:hover {
	color: #00afa6;
	text-decoration:none;
	-o-transition: .2s ease-in-out;
	-ms-transition: .2s ease-in-out;
	-moz-transition: .2s ease-in-out;
	-webkit-transition: .2s ease-in-out;
	transition: .2s ease-in-out;
	cursor: auto;
}
ul.lineup li.lul {
	font-size:15px;
	line-height:15px;
	display:block;
}
ul.lineup li.lu0 {
	font-size:44px;
	line-height:24px;
	display:block;
	clear:both;
	margin: 5px auto 10px auto;
}
ul.lineup li.lu0 .sup {
	font-size: 22px;
	vertical-align: 70%;
}
ul.lineup li.lu1 {
	font-size:44px;
	line-height:24px;
	margin: 0 auto;
}
ul.lineup li.lu2 {
	font-size:28px;
	line-height:16px;
	margin: 0 auto;
}
ul.lineup li.lu2 .sup {
	font-size: 13px;
	vertical-align: 70%;
}
ul.lineup li.lu3 {
	font-size:20px;
	line-height:6px;
	margin: 0 auto;
}
ul.lineup li.break {
	font-size:5px;
	line-height:5px;
	height: 5px;
	font-weight: lighter;
	display:block;
	clear:both;
	margin: 0 0;
}
ul.lineup li.pbreak {
	font-size:25px;
	line-height:25px;
	height: 25px;
	font-weight: lighter;
